        <script>
            /* Internal tracking for biolink links */
            $('[data-link-url]').on('click', event => {


                let url = $(event.currentTarget).data('link-url');
                $.ajaxSetup({
                    "headers": {
                        'X-CSRF-TOKEN': 'WeyqqO8ai8hPEIKBCeHcDbeNAALrQYDLFX8wW2gl'
                    }
                })
                $.ajax({
                    url: "https://zaap.bio/track-link",
                    type: "POST",
                    data: {
                        url: url,
                    },
                });

            });
